{
	"$schema": "https://raw.githubusercontent.com/CodeEditorLand/Tauri/Current/tooling/cli/schema.json",
	"productName": "Editor",
	"version": "0.0.0",
	"identifier": "app.editor.land",
	"build": {
		"beforeDevCommand": "pnpm Start",
		"devUrl": "http://localhost:9999",
		"beforeBuildCommand": "pnpm prepublishOnly",
		"frontendDist": "../Target"
	},
	"app": {
		"macOSPrivateApi": true,
		"windows": [
			{
				"title": "interface",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"assetProtocol": {
				"enable": false,
				"scope": []
			},
			"capabilities": [],
			"csp": null,
			"dangerousDisableAssetCspModification": false,
			"freezePrototype": false,
			"pattern": {
				"use": "isolation",
				"options": {
					"dir": "../Target/"
				}
			}
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
